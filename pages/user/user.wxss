.unlogin-body {
  background-color: #fff;
  height: 100vh;
  width: 100vw;
}
.unlogin {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 40%;
}

.unlogin .unlogin-image {
  width: 150rpx;
  height: 150rpx;
}

.unlogin .unlogin-btn {
  margin-top: 50rpx;
  width: 80%;
  background-color: #f1a417;
  color: #fff;
  height: 100rpx;
  line-height: 2.1;
}

.info {
  width: 80%;
  background-color: #f1a417;
  display: flex;
  border-radius: 10rpx;
  margin: 0 auto;
  padding: 50rpx 40rpx;
  align-items: center;
  position:relative;
  top:10rpx;
}

.avatar_wrapper{

  display: flex;

  align-items: center; /* 居中显示头像 */
  /*border: none; /* 去掉按钮边框 */
  background: transparent; /* 去掉按钮背景 */
}

.avatarUrl {
width: 100rpx;
height: 100rpx;
margin-left: auto;

}

.info .nickName {
position: relative; left: -250rpx; top: 0rpx; width: 166rpx; display: flex; box-sizing: border-box; height: 54rpx;
color:rgb(7, 7, 7);
}

.cell-body {
  position: relative;
  top:40rpx;
}

.cell-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  width: 100%;
  top: 15rpx;
}

.cell {
  display: flex;
  justify-content: space-between;
  width: 85%;
  background-color: #fff;
  padding: 30rpx 20rpx;
  border-radius: 20rpx;
  align-items: center;
  margin-bottom: 20rpx;
}

.cell-left {
  display: flex;
  align-items: center;
}

.cell-left .cell-left-image {
  width: 50rpx;
  height: 50rpx;
}

.cell-left .cell-left-text {
  margin-left: 20rpx;
}

.cell-right .cell-right-image {
  width: 30rpx;
  height: 30rpx;
}